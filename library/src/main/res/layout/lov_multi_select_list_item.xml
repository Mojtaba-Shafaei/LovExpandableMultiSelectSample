<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.AppCompatCheckedTextView xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/des"
  android:layout_width="match_parent"
  android:layout_height="64dp"
  android:paddingLeft="16dp"
  android:paddingRight="16dp"
  android:background="@drawable/lov_multi_select_list_item_background"
  android:checkMark="@drawable/lov_multi_select_check_mark"
  android:checked="false"
  android:drawablePadding="16dp"
  android:ellipsize="end"
  android:foreground="?attr/selectableItemBackground"
  android:gravity="center_vertical|right"
  android:lineSpacingMultiplier="1.1"
  android:maxLines="2"
  android:minHeight="@dimen/lov_multi_select_listPreferredItemHeightSmall"
  android:text=""
  android:textColor="@color/lov_multi_select_colors_text_dark"
  android:textSize="14sp"
  app:layout_constraintBottom_toBottomOf="parent"
  app:layout_constraintLeft_toLeftOf="parent"
  app:layout_constraintRight_toRightOf="parent"
  app:layout_constraintTop_toTopOf="parent"
  tools:text="@tools:sample/lorem"/>
